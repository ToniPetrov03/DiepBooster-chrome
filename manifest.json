{
  "name": "DiepBooster",
  "version": "1.0",
  "description": "Auto stack the bullets in any reload speed!",
  "content_scripts": [
    {
      "matches": [
        "https://diep.io/*"
      ],
      "js": [
        "constants.js",
        "keyEvents.js",
        "predatorStacking.js",
        "group135DegreesStacking.js",
        "updateTanksInfo.js",
        "stackingBulletsControls.js",
        "scriptSwitcher.js"
      ]
    }
  ],
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/booster16.png",
      "32": "images/booster32.png",
      "48": "images/booster48.png",
      "128": "images/booster128.png"
    }
  },
  "icons": {
    "16": "images/booster16.png",
    "32": "images/booster32.png",
    "48": "images/booster48.png",
    "128": "images/booster128.png"
  },
  "manifest_version": 3
}
